<!DOCTYPE html>
<html>
	<head>
	<title><%= title %>| Node Tunes Artists Page </title>
	</head>
<body>

	<table>
		<thead>
			<tr>
				<th>Artist</th>
				<th>Genre</th>
				<th>Wiki</th>
			</tr>
		</thead>
    <tbody>
      <% artists.forEach(function (artist) { %>
        <tr>
          <td><%= artist.artists %></td>
          <td><%= artist.genre %></td>
          <td><%= artist.wiki %></td>
          <td>
             	<form action="/artist/<%= artist._id %>/submit" method="POST">
   						</form>
   				</td>
        </tr>
      <% }); %>
     </tbody>
	</table>
</body>
</html>

